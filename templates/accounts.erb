[<%= @name %>]
enabled=<%= @enabled %>
jid=<%= (defined? @jid) ? @jid : @name %>
resource=<%= @resource %>
muc.service=<%= (defined? @muc) ? @muc : ((defined? @jid) ? "conference.#{@jid.split('@')[1]}" : "conference.#{@name}") %>
muc.nick=<%= (defined? @nick) ? @nick : @name %>
presence.last=online
presence.login=<%= @status %>
priority.online=<%= (defined? @priority_online) ? @priority_online : 0 %>
priority.chat=<%= (defined? @priority_chat) ? @priority_chat : 0 %>
priority.away=<%= (defined? @priority_away) ? @priority_away : 0 %>
priority.xa=<%= (defined? @priority_xa) ? @priority_xa : 0 %>
priority.dnd=<%= (defined? @priority_dnd) ? @priority_dnd : 0 %>
<% if (defined? @port) -%>
port=<%= @port %>
<% end -%>
<% if (defined? @password) -%>
password=<%= @password %>
<% end -%>
<% if (defined? @eval_password) -%>
eval_password=<%= @eval_password %>
<% end %>
